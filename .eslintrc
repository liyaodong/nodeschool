{
  "ecmaFeatures": {
    "jsx": true,
    "arrowFunctions": true,
    "blockBindings": true,
    "generators": true,
    "forOf": true
  },
  "rules": {
    "strict": 0,
    "no-underscore-dangle": 0,
    "no-unused-vars": 0,
    "no-unused-expressions": 0,
    "curly": 0,
    "no-multi-spaces": 0,
    "key-spacing": 0,
    "no-return-assign": 0,
    "consistent-return": 0,
    "no-shadow": 0,
    "no-comma-dangle": 0,
    "no-use-before-define": 0,
    "no-mixed-requires": 0,
    "no-trailing-spaces": 0,
    "no-empty": 0,
    "no-alert": 0,
    "new-parens": 0,
    "no-cond-assign": 0,
    "quotes": [2, "single", "avoid-escape"],
    "camelcase": 0,
    "new-cap": [1, { "capIsNew": false }],
    "no-undef": 2
  },
  "env": {
    "node": true,
    "browser": true
  },
  "globals": {
    "$": true,
    "jQuery": true,
    "GeekPark": true,
    "APP": true,
    "wx": true,
    "_": true,
    "Swiper": true,
    "AMap": true,
    "React": true,
  }
}
